f      = "clm ~ I(exposure)+I(exposure^2)+I(sqrt(exposure))"
eval_modelo <-function(f,datos)
{
print(f)
modelo      = glm(formula=f, family = binomial(link = "logit"), data=datos )
resumen     = data.frame(
formula     = as.character(modelo$formula),
AIC         = AIC(modelo),
logLik      = as.numeric(logLik(modelo)),
Dev         = modelo$deviance
)
resumen
}
eval_modelo(f,datos)
